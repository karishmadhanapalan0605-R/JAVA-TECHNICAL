import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
      int x = sc.nextInt();
      if(x > 0){
        System.out.println("POSITIVE");
      }
      else if (x < 0) {
        System.out.println("NEGATIVE");
      }
      else{
        System.out.println("ZERO");
      }
  }
}
