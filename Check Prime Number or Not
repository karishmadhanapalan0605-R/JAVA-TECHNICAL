import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int i , sum;
        i=2;
        sum = 0;
        while(i<=a)
        {
        if(a%i == 0)
            {
                sum++;
            }
            i++;
        }
       if(sum == 1)
       {
           System.out.println("Prime number");
       }
       else
       {
          System.out.println("Not a Prime number"); 
       }
    } 
            
        }




****************************************************************
import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int i , sum;
        i=2;
        sum = 0;
        while(i<=a)
        {
        if(a%i == 0)
            {
                sum++;
            }
            i++;
        }
       System.out.println(sum==1?"prime":"not prime");
    }
}
