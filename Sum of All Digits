import java.util.*;

public class Main {
    public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
  int rev = 0;
  int rem;
  while(a>0){
    rem = a %10;
    rev = rev+rem;
    a/=10;
  }
  System.out.println(rev);
      
    }
} sum of all the digit
