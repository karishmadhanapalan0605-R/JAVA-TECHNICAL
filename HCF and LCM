import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
    int b = sc.nextInt();
    int hcf = 1;
    for(int i = 1; i <=a && i<=b;i++){
      if(i%a==0&&%i==0){
        hcf=i;
        
      }
    }
    int lcm;
    if(a==0||b==0){
      lcm=0;
    }
    else{
      lcm=(a*b)/hcf;
    }
    System.out.println(hcf);
    System.out.println(lcm);
    }
  }
