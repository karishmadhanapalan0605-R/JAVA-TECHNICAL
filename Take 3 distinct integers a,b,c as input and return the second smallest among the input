import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
      int a = sc.nextInt();
      int b = sc.nextInt();
      int c = sc.nextInt();
      int sec;
      if((a > b && a < c) || (a > c && a < b)){
        sec = a;
      }
      else if((b > a && b < c) || (b > c && b < a)){
        sec = b;
      }
      else{
        sec = c;
      }
      System.out.println(sec);
  }
}
