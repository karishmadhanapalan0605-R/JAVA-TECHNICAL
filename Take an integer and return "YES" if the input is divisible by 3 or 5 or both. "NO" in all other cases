import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
      int b = sc.nextInt();
      if(b % 3 == 0 || b % 5 == 0) 
      {
        System.out.println("YES");
      }
      else{
        System.out.println("NO");
      }
  }
}
